{
  "expo": {
    "name": "InovaMobile",
    "slug": "InovaMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/senaiInova.png",
    "splash": {
      "image": "./src/assets/images/maisVagasSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DA251C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "build": {
        "babel": {
          "include": ["react-router-native"]
        }
      }
    }
  }
}
